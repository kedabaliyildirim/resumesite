    "nuxt": "^3.5.0",
    "nuxt-mail": "^4.0.1",
    "nuxt-security": "^0.13.1",
    "push-dir": "^0.4.1"
  },
  "overrides": {
    "vue": "latest"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.4.11",
    "pinia": "^2.1.3"
